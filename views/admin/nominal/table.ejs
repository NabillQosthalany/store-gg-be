<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No.</th>
      <th>Nama Koin</th>
      <th>Jumlah Koin</th>
      <th>Harga Koin</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < nominal.length; i++) { %>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= nominal[i].coinName %></td>
      <td><%= nominal[i].coinQuantity %></td>
      <td><%= nominal[i].price %></td>
      <td>
        <form
          action="/nominal/delete/<%= nominal[i]._id %>?_method=DELETE"
          method="POST"
        >
          <a class="btn btn-warning" href="/nominal/edit/<%= nominal[i]._id %>">
            <i class="fa fa-pencil-alt"> Ubah </i>
          </a>
          <button class="btn btn-danger">
            <i class="fa fa-trash">Hapus</i>
          </button>
        </form>
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
